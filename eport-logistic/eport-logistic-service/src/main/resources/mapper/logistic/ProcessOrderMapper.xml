            <if test="logisticGroupId != null ">logistic_group_id = #{logisticGroupId},</if>
            <if test="serviceType != null ">service_type = #{serviceType},</if>
            <if test="invoiceNo != null  and invoiceNo != ''">invoice_no = #{invoiceNo},</if>
