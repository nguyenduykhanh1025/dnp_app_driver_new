    <sql id="selectProcessOrderVo">
        select id, shipment_id, logistic_group_id, service_type, invoice_no, pay_type, sztp, modee, consignee, truck_co, 
        tax_code, bl_no, booking_no, pickup_date, vessel, voyage, before_after, year, cont_number, status, 
        result, data, create_by, create_time, update_by, update_time, robot_uuid, ssr_code, sys_user_id, post_process_id, runnable, order_no, process_data from process_order
    </sql>

    <select id="selectProcessOrderList" parameterType="ProcessOrder" resultMap="ProcessOrderResult">
        <include refid="selectProcessOrderVo"/>
        <where>  
