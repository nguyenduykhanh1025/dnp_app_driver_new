		</selectKey>
        insert into sys_robot
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null and id != 0">id,</if>
            <if test="uuId != null  and uuId != ''">uuid,</if>
            <if test="status != null  and status != ''">status,</if>
